<section id="main-cat">	<div class="container">	    <div class="row">	    {widget_catalog_data	        conf = [	            'context' => ['category' => 'product'],	            'select' => [{getlang} => {#id_mainCat#}]	            ]	        assign="mainCat"	    }	    {if $mainCat[0].subdata}	        <div class="center-gallery-xs-6 center-gallery-md-4">	        {include file="home/loop/category.tpl" data=$mainCat[0].subdata classCol='col-xs-6 col-md-4' effect="ming"}	        </div>	    {/if}	    </div>	</div></section>